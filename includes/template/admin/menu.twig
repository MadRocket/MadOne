<div class="well">
    <ul class="unstyled">
        {% for i in menuItems %}
        <li>
            {% if i.selected %}
            <a class="active">{{ i.title }}</a>
            {% else %}
            <a href="{{ i.uri }}">{{ i.title }}</a>
            {% endif %}
        </li>
        {% endfor %}
    </ul>
</div>